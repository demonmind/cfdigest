  [1m[35mSQL (0.2ms)[0m  INSERT INTO `posts` (`created_at`, `posted_at`, `text`, `thread_id`, `updated_at`, `username`) VALUES ('2013-10-05 23:42:22', '2013-10-05 23:42:22', ' Hi Vera,<br> <br> You can add me to list of Cayston lovers! <img src=\\imagessmiliessmile.png\\\" border=\\\"0\\\" alt=\\\"\\\" title=\\\"Smile\\\" class=\\\"inlineimg\\\"><br> <br> I cant tolerate Tobi', 4585, '2013-10-05 23:42:22', NULL)
