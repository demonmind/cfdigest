  [1m[35mSQL (0.9ms)[0m  INSERT INTO `posts` (`created_at`, `posted_at`, `text`, `thread_id`, `updated_at`, `username`) VALUES ('2013-10-05 23:39:05', '2013-10-05 23:39:05', ' <div class=\\bbcode_container\\\"> <div class=\\\"bbcode_quote\\\"> <div class=\\\"quote_container\\\"> <div class=\\\"bbcode_quote_container\\\"><div> <div class=\\\"bbcode_postedby\\\"> <img src=\\\"imagesmiscquote_icon.png\\\" alt=\\\"Quote\\\"> Originally Posted by <strong>epicurus<strong> <a href=\\\"showthread.php?s=aa8e66b7b57b396b34d34a6008e4ed35&amp', 7536, '2013-10-05 23:39:05', NULL)
